<h1>Students</h1>

<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    <% @students.each do |student| %>
        <tr>
            <td><%= link_to student.first_name, student_path(student) %></td>
            <td><%= student.last_name %></td>
        </tr>
    <% end %>
</table>