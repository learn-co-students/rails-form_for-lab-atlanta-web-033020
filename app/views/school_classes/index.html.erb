<h1>School Classes</h1>

<table>
    <tr>
        <th>Title<th>
        <th>Room Number</th>
    </tr>
    <% @school_classes.each do |school_class| %>
        <tr>
            <td><%= link_to school_class.title, school_class_path(school_class) %></td>
            <td><%= school_class.room_number %></td>
        </tr>
    <% end %>
</table>